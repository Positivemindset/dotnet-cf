{
    "scriptFile": "DotNetCf.dll",
    "entryPoint": "DotNetCf.Function.Run",
    "bindings": [
      {
        "name": "req",
        "type": "httpTrigger",
        "direction": "in",
        "methods": [ "get", "post" ],
        "authLevel": "anonymous"
      },
      {
        "name": "$return",
        "type": "http",
        "direction": "out"
      }
    ]
  }
  